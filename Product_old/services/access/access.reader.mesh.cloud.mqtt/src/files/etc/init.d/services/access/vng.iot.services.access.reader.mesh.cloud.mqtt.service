[Unit]
Description=Access reader mesh local
After=network.target

[Service]
ExecStartPre=/bin/sleep 15
Type=forking
ExecStart = /etc/init.d/vng.services.access.reader.mesh.cloud.mqtt start
ExecStop = /etc/init.d/vng.services.access.reader.mesh.cloud.mqtt stop
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target
