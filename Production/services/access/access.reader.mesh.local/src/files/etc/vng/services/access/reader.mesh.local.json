{
    "name" : "vng.iot.services.access.reader.local",
    "id": "70:b3:d5:8d:88:20",
    "synchronize": {
        "events": 256,
        "timeout": -1
    },
    "settings": {
        "net": {
            "processors":[
            ],

            "servers": [
            ],

            "listeners": [
            ],

            "channels":[{
                    "id": "local_btn",
                    "protocol": "MQTT",
                    "settings": {
                        "host": "localhost",
                        "port": 1883
                    }
                }
            ],

            "publishers": [{
                    "id": "local",
                    "channel": "local_btn",
                    "topic": "tag.mesh.local"
                }
            ],

            "subscribers": [{
                    "id": "local_btn",
                    "channel": "local_btn",
                    "topic": "bluetooth.receive"
                }
            ]
        },
        "storage": {
            "storers": [
                {
                    "id": "local",
                    "driver": "SQLITE",
                    "settings": {
                        "path": "/etc/vng/services/synchronizer/reader.sqlite"
                    }
                },{
                    "id": "door",
                    "driver": "SQLITE",
                    "settings": {
                    "path": "/etc/vng/services/synchronizer/door.sqlite"
                }
            }
            ]
        },
        "main": {
            "reader_storer"     : "local",
